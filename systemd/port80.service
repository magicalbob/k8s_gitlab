[Unit]
Description=Kubectl Port Forward 80
Wants=network-online.target
After=network-online.target

[Service]
User=vagrant
Group=vagrant
Type=simple
ExecStart=/bin/bash -c "kubectl port-forward service/gitlab -n gitlab --address 192.168.0.10 8080:80"
Restart=always
RestartSec=1s

[Install]
WantedBy=multi-user.target
